<tr id="<%= dom_id contact %>">
  <td><%= link_to contact.first_name + " " + contact.last_name, contact %></td>  
  <td><%= contact.email %></td>  
  <td><%= contact.phone %></td>  
  <td><%= link_to "Delete", contact, data: {turbo_method: :delete, turbo_confirm: "Are you sure?" }, class:"btn btn-outline-danger btn-sm" %></td>  
</tr>




