<h1 class="display-6">Contacts</h1>
<br />

<div class=>
<table class="table table-striped table-bordered table-hover">
  <thead class="table-dark">
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th> </th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <%= render contact %>
    <% end %>
  </tbody>
</table>
 
<%= link_to "New", new_contact_path, class:"btn btn-outline-success" %>



